{
    "builds": [
        {
        "src": "projeto_cad_usuarios/settings.py",
        "use": "@vercel/python"
        },

        {
            "src": "projeto_cad_usuarios/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        { "handle": "filesystem" },
        {
            "src": "/(.*)",
            "dest": "projeto_cad_usuarios/wsgi.py"
        }
    ]
}